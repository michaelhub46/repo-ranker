# Test Environment Configuration
NODE_ENV=test
PORT=3002

# GitHub Configuration for Testing
GITHUB_API_URL=https://api.github.com
GITHUB_TIMEOUT=5000
GITHUB_MAX_RETRIES=1
GITHUB_RATE_LIMIT_BUFFER=10
# No GitHub token for tests - use unauthenticated requests
GITHUB_TOKEN=

# Cache Configuration
CACHE_TTL=60

# Rate Limiting (more lenient for testing)
RATE_LIMIT_PER_MINUTE=1000

# CORS (allow all for testing)
CORS_ORIGINS=*

# API Configuration
API_PREFIX=api
